{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.mts",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/vite.config.ts",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/server.mts"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/dist/$1"
      }
    ]
  }
  