{
    "version": 2,
    "builds": [
      {
        "src": "backend/server.mts",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.mts"
      }
    ]
  }
  